{
    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "program": "${workspaceFolder}/server/server.ts",
            "preLaunchTask": "tsc: build - server/tsconfig.json",
            "outFiles": ["${workspaceFolder}/server/out/**/*.js"]            
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch wearable-store-service-ft",
            "program": "${workspaceFolder}/wearable-store-service-ft/index.ts",
            "preLaunchTask": "tsc: build - wearable-store-service-ft/tsconfig.json",
            "cwd":"${workspaceFolder}/wearable-store-service-ft",
            "outFiles": ["${workspaceFolder}/wearable-store-service-ft/dist/**/*.js"]            
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch chain-playfab",
            "program": "${workspaceFolder}/chain-playfab/index.ts",
            "preLaunchTask": "tsc: build - chain-playfab/tsconfig.json",
            "cwd":"${workspaceFolder}/chain-playfab",
            "outFiles": ["${workspaceFolder}/chain-playfab/dist/**/*.js"]            
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Webpack",
            "program": "${workspaceFolder}/node_modules/webpack/bin/webpack.js",
            "env": {}
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch group-match-service",
            "program": "${workspaceFolder}/group-match-service/index.ts",
            "preLaunchTask": "tsc: build - group-match-service/tsconfig.json",
            "cwd":"${workspaceFolder}/group-match-service",
            "outFiles": ["${workspaceFolder}/group-match-service/out/**/*.js"]            
        },

    ]
}