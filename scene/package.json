{
  "name": "dcl-project",
  "version": "1.0.0",
  "description": "My new Decentraland project",
  "scripts": {
    "start": "dcl start",
    "build": "build-ecs",
    "watch": "build-ecs --watch",
    "deploy:now": "dcl export && now export",
    "ecs:install": "npm install --save-dev decentraland-ecs@latest",
    "ecs:install-next": "npm install --save-dev decentraland-ecs@next",
    "bundle": "webpack --config ./build/webpack.config.js --env.production",
    "bundle-watch": "webpack --watch --config ./build/webpack.config.js --env.development",
    "bundle-metazone": "webpack --config ./build/metazone.webpack.config.js --env.development",
    "test": "env TS_NODE_PROJECT='./test/tsconfig.json' mocha -r ts-node/register 'test/**/*.ts'"
  },
  "dependencies": {
    "@compicactus/dcl-scene-utils": "^0.3.3",
    "@dcl/crypto-scene-utils": "^1.1.1",
    "@dcl/ecs-scene-utils": "^1.7.5",
    "@dcl/l2-scene-utils": "^1.0.7",
    "@dcl/npc-scene-utils": "^1.5.1",
    "@dcl/ui-scene-utils": "^1.8.4",
    "@ohmyverse/oh-my-pixel-utils": "^1.0.6",
    "@ohmyverse/time-color": "^1.0.7",
    "cannon": "^0.6.2",
    "colyseus.js": "^0.14.13",
    "decentraland-transactions": "~1.24.0",
    "dexie": "^3.0.3",
    "eth-connect": "^6.0.2",
    "localbase": "^0.7.5",
    "padleft": "^0.2.1",
    "playfab-sdk": "^2.68.210511",
    "playfab-web-sdk": "^1.87.210521",
    "ts-transform-define": "^0.1.10"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "decentraland": "^3.14.2",
    "decentraland-ecs": "^6.12.3",
    "mana-fever-utils": "0.0.16",
    "mocha": "^9.1.3",
    "ts-loader": "^7.0.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.11"
  },
  "bundledDependencies": [
    "@compicactus/dcl-scene-utils",
    "@dcl/crypto-scene-utils",
    "@dcl/ecs-scene-utils",
    "@dcl/l2-scene-utils",
    "@dcl/npc-scene-utils",
    "@dcl/ui-scene-utils",
    "@ohmyverse/time-color",
    "colyseus.js",
    "decentraland-transactions",
    "eth-connect",
    "@ohmyverse/oh-my-pixel-utils"
  ],
  "bundleDependencies": [
    "@compicactus/dcl-scene-utils",
    "@dcl/crypto-scene-utils",
    "@dcl/ecs-scene-utils",
    "@dcl/l2-scene-utils",
    "@dcl/npc-scene-utils",
    "@dcl/ui-scene-utils",
    "@ohmyverse/time-color",
    "colyseus.js",
    "decentraland-transactions",
    "eth-connect",
    "@ohmyverse/oh-my-pixel-utils"
  ]
}
